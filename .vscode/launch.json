    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "${workspaceRoot}${pathSeparator}src${pathSeparator}ui${pathSeparator}app.py",
                "FLASK_ENV": "development",
                "PYTHONPATH": "${env:PYTHONPATH};${workspaceRoot}${pathSeparator}src"
            },
            "args": [
                "run"
            ],
            "jinja": true,
            "justMyCode": true
        }
    ]
}